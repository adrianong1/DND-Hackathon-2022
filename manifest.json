{
    "name": "Acronym Helper",
    "description": "Chrome Extension that helps with DND related acronyms",
    "version": "1.0.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action":{
        "default_popup":"popup.html"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "contextMenus"
    ]
}