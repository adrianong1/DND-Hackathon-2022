{
    "name": "Acronym Helper",
    "description": "Chrome Extension that helps with DND related acronyms",
    "version": "1.0.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page":"./options.html",
    "browser_action":{
        "default_popup":"popup.html"
    },
    "permissions":[
        "tabs","https://google.com/*"
    ]
}